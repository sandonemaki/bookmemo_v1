    <div class="bl_maintitle">
      <h1 class="el_lv1hedding">
        本の登録
      </h1>
    </div>

    <%= form_tag("/books/create") do %>
      <p>本のタイトル</p>
      <input name="book_title" value="<%book.book_title %>"></input>
